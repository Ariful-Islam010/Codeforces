#include<iostream>
#include<cctype>
using namespace std;
int main()
{
    int n,i;
    string s;
    cin>>n;
    for(i=0;i<n;i++)
    {
        cin>>s;
        if(s=="YES" || s=="Yes"||s=="yEs"||s=="yeS"||s=="YEs"||s=="yES"||s=="YeS"||s=="yes")
        cout<<"YES\n";
        else
        cout<<"NO\n";
    }
    return 0;
}
